<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.laodeng.crm.workbench.dao.ContactsActivityRelationDao">

    <insert id="save" parameterType="ContactsActivityRelation">

        insert into tbl_contacts_activity_relation(id,contacts_id,activity_id) values(
            #{id},
            #{contacts_id},
            #{activity_id}
        )

    </insert>

</mapper>